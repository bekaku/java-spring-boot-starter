<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bekaku.api.spring.mapper.AccessTokenMapper">
    <update id="updateLastestActive">
        update access_token
        <set>
            lastest_active = #{lastestActive}
        </set>
        where id = #{id}
    </update>
</mapper>
